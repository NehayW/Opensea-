     <link rel="stylesheet" href="style/items.css">
    <title>Listed Item</title>
</head>

<body>
    <%-include('navbar')-%>
    <div class="container-fluid">
        <div style="position:relative ;" >
            <div class="d-flex p-4" style="background-color:#4E54C8;">
                <img src="image/avatar.png" alt="profile" width="80px" height="80px;">
                <div class="p-2"><span class="text-white avatar">@Foxnet_creater</span> <br>
                    <p class="avatar1">Joined July 2022</p>
                </div>
            </div>
        </div>

        <div class="container">
            <div class="row indexrow">
            <div class="col-sm-12">
                <div class="row p-4">
                    <div class="d-flex bd-highlight" style="flex-wrap:wrap ;align-items: center;">
                        <div class="myitems p-2 flex-grow-1 bd-highlight headcard">My Items</div>
                        <div class="myitems1 p-2 bd-highlight">All(5)</div>
                        <div class="myitems1 p-2 bd-highlight">Sold(5)</div>
                        <div class="myitems1 p-2 bd-highlight">Sold(5)</div>
                    </div>

                    <% for(i in data) {%>
                        <div class="col-sm-4 col-md-4">
                            <div class="card mt-2">
                                <img src="<%= data[i].image%>" class="card-img-top" alt="...">
                                <div class="top-right" data-bs-toggle="tooltip" data-bs-placement="left"
                                    title="Edit"><img src="image/heart.png" alt="like" width="25px;"></div>
                                <div class="card-body">
                                    <h6 class="card-title p-0"><%= data[i].name%></h6>
                                    <div class="d-block p-0">
                                        <div class="text-secondary">Current price with fee</div>

                                        <div class="fs-bold d-flex"><%= data[i].totalPrice%> ETHs &nbsp; <div class="text-secondary dollar"> 
                                                </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="card-footer bg-white">
                                    <div class="d-flex justify-content-start  mt-2">
                                        <img class="rounded-circle" src="image/profile.jpg" alt="clientname" width="30px;">
                                        <div class="days">
                                            <small id="itemid"><%= data[i].itemId%></small>
                                            &nbsp; <a href="#" class="purchase">#<%= data[i].name%></a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        
                        </div>
                    <% } %>

                    <div class="col-sm-4 col-md-4">
                            <div class="card mt-2">
                                <img src="image/n3.jpg" class="card-img-top" alt="...">
    
                                <div class="top-right" data-bs-toggle="tooltip" data-bs-placement="left"
                                    title="Edit"><img src="image/heart.png" alt="like" width="25px;"></div>
                                <div class="card-body">
                                    <h6 class="card-title p-0">Lorem consectetur adipisicing elit.</h6>
    
                                    <div class="d-block p-0">
                                        <div class="text-secondary">Current price with fee</div>
                                        <div class="fs-bold d-flex">0.5 ETHs &nbsp; <div class="text-secondary dollar"> 
                                                </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="card-footer bg-white">
                                    <div class="d-flex justify-content-start  mt-2">
                                        <img class="rounded-circle" src="image/profile.jpg" alt="clientname" width="30px;">
                                        <div class="days">&nbsp; #Heyalaurin
                                        </div>
                                    </div>
                                </div>
                            </div>
                        
                    </div>
                    <div class="col-sm-4 col-md-4">
                        <div class="card mt-2">
                            <img src="image/n2.jpg" class="card-img-top" alt="...">
                            <div class="top-left text-light bg-dark clock">&nbsp; &#x1F551; 06:55:12 &nbsp;
                            </div>
                            <div class="top-right" data-bs-toggle="tooltip" data-bs-placement="left"
                                title="Edit"><img src="image/heart.png" alt="like" width="25px;"></div>
                                <div class="card-body">
                                    <h6 class="card-title p-0">Lorem consectetur adipisicing elit.</h6>
                                    
                                    <div class="d-block p-0">
                                    <div class="text-secondary">Current price with fee</div>
                                    <div class="fs-bold d-flex">0.5 ETHs &nbsp; <div class="text-secondary dollar"> 
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="card-footer bg-white">
                                <div class="d-flex justify-content-start mt-2 ">
                                    <img class="rounded-circle" src="image/profile.jpg" alt="clientname" width="30px;">
                                    <div class="days">&nbsp; #Heyalaurin
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-4 col-md-4">
                        <div class="card mt-2">
                            <img src="image/img.jpg" class="card-img-top" alt="...">
                            
                            <div class="top-right" data-bs-toggle="tooltip" data-bs-placement="left"
                                title="Edit"><img src="image/heart.png" alt="like" width="25px;"></div>
                                <div class="card-body">
                                    <h6 class="card-title p-0">Lorem consectetur adipisicing elit.</h6>
                                    
                                    <div class="d-block p-0">
                                        <div class="text-secondary">Current price with fee</div>
                                        <div class="fs-bold d-flex">0.5 ETHs &nbsp; <div class="text-secondary dollar"> 
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="card-footer bg-white">
                                    <div class="d-flex justify-content-start mt-2 ">
                                        <img class="rounded-circle" src="image/profile.jpg" alt="clientname" width="30px;">
                                        <div class="days">&nbsp; #Heyalaurin
                                        </div>
