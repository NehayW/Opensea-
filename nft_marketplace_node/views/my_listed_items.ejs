<%-include('navbar')-%>
<h1>My Listed Items</h1>

<div class="container">
<div class="row">
  <% for(i in data) {%>
    <div class="col-md-4">
      <div class="card" style="width: 18rem;">
        <img src="<%= data[i].image%>" class="card-img-top" alt="...">
        <div class="card-body">
          <h5 class="card-title"><%= data[i].name%></h5>
          <p class="card-text"><%= data[i].description%></p>
          <small id="itemid"><%= data[i].itemId%></small>
          <h6>Price with fee : <%= data[i].totalPrice%></h6>
          <a href="#" class="btn btn-primary purchase">Buy at<%= data[i].totalPrice%></a>
        </div>
      </div>
    </div>
  <% } %>
  </div>
  </div>
<script src="https://code.jquery.com/jquery-3.3.1.min.js"
    integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous"></script>
<script type="text/javascript" src="/script/web.js"></script>


